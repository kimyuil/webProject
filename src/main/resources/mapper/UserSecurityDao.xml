<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user">
     <select id="selectUserById" resultType="com.spring.webProject.dto.UserSecurityDto" parameterType="String">
        
            SELECT
                *
            FROM
                user
            WHERE
                uId=#{uId}
        
    </select>
 </mapper>

